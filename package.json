{
  "name": "jheeebert-redux-store",
  "version": "1.0.0",
  "description": "An e-commerce platform utilizing Redux for state management.",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm install && cd ../client && npm install",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm run build"
  },
  "keywords": [
    "ecommerce",
    "redux",
    "state management"
  ],
  "author": "Joshua Hebert",
  "repository": {
    "type": "git",
    "url": "https://github.com/jheebert/redux-store.git"
  },
  "bugs": {
    "url": "https://github.com/jheebert/redux-store/issues"
  },
  "homepage": "https://github.com/jheebert/redux-store#readme",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^5.1.0"
  },
  "dependencies": {
    "if-env": "^1.0.4",
    "redux": "^4.2.1",
    "sweetalert2": "^11.7.31",
    "sweetalert2-react-content": "^5.0.7"
  }
}
